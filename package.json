{
  "name": "@adobe/helix-static",
  "version": "1.11.24",
  "private": true,
  "description": "Helix micro service for serving static files from GitHub",
  "main": "src/index.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "semantic-prepare": "node ./build/prepare.js",
    "commit": "git-cz",
    "lint": "eslint .",
    "test": "nyc mocha && npm run lint",
    "test-ci": "npm run lint && nyc mocha --reporter mocha-junit-reporter && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "wsk-builder -v",
    "deploy": "wsk-builder -v --deploy --test=/_status_check/healthcheck.json",
    "deploy-test": "wsk-builder -v --deploy --test=/_status_check/healthcheck.json --pkgVersion=test",
    "deploy-sequences": "wsk-builder --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "wsk-builder -v --deploy --test=/_status_check/healthcheck.json --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci"
  },
  "wsk": {
    "name": "helix-services/static@${version}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-static.git"
  },
  "keywords": [
    "helix"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-static/issues"
  },
  "homepage": "https://github.com/adobe/helix-static#readme",
  "dependencies": {
    "@adobe/helix-epsagon": "1.3.10",
    "@adobe/helix-log": "4.5.1",
    "@adobe/helix-shared": "7.5.0",
    "@adobe/helix-status": "7.1.3",
    "@adobe/openwhisk-action-logger": "2.2.0",
    "@adobe/openwhisk-action-utils": "4.2.2",
    "@babel/core": "7.10.2",
    "mime-types": "2.1.27",
    "object-hash": "2.0.3",
    "postcss": "7.0.32",
    "postcss-url": "8.0.0",
    "postcss-value-parser": "4.1.0",
    "sanitizer": "0.1.3",
    "uri-js": "4.2.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.1",
    "@adobe/helix-ops": "1.11.2",
    "@adobe/openwhisk-action-builder": "2.12.0",
    "@pollyjs/adapter-node-http": "4.3.0",
    "@pollyjs/core": "4.3.0",
    "@pollyjs/persister-fs": "4.3.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "codecov": "3.7.0",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "dotenv": "8.2.0",
    "eslint": "7.2.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.21.1",
    "ghooks": "2.0.4",
    "lint-staged": "10.2.9",
    "mocha": "7.2.0",
    "mocha-junit-reporter": "2.0.0",
    "nock": "12.0.3",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "semantic-release": "17.0.8"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
