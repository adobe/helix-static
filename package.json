{
  "name": "@adobe/helix-static",
  "version": "1.3.1",
  "private": true,
  "description": "Helix micro service for serving static files from GitHub",
  "main": "src/index.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "semantic-prepare": "node ./build/prepare.js",
    "commit": "git-cz",
    "lint": "eslint .",
    "test": "nyc mocha && npm run lint",
    "test-ci": "npm run lint && nyc mocha --reporter mocha-junit-reporter && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "wsk-builder -v",
    "deploy": "wsk-builder -v --deploy --test -p EPSAGON_TOKEN=$EPSAGON_TOKEN",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "wsk": {
    "name": "helix-services/static@1.3.1",
    "static": []
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/adobe/helix-static.git"
  },
  "keywords": [
    "helix"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-static/issues"
  },
  "homepage": "https://github.com/adobe/helix-static#readme",
  "dependencies": {
    "@adobe/helix-pingdom-status": "3.1.0",
    "@adobe/helix-shared": "2.2.1",
    "@adobe/openwhisk-action-utils": "^1.0.2",
    "@babel/core": "^7.5.5",
    "epsagon": "^1.33.0",
    "mime-types": "^2.1.24",
    "object-hash": "^1.3.1",
    "postcss": "^7.0.17",
    "postcss-url": "^8.0.0",
    "postcss-value-parser": "^4.0.2",
    "sanitizer": "^0.1.3",
    "snyk": "^1.216.4",
    "uri-js": "^4.2.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "^1.0.0",
    "@adobe/openwhisk-action-builder": "^2.0.0",
    "@pollyjs/adapter-node-http": "^2.6.2",
    "@pollyjs/core": "^2.6.2",
    "@pollyjs/persister-fs": "^2.6.2",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/exec": "^3.3.6",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.2",
    "ajv": "^6.10.2",
    "archiver": "^3.1.1",
    "chalk": "^2.4.1",
    "codecov": "^3.5.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "ghooks": "^2.0.4",
    "lint-staged": "^9.2.1",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "nock": "^11.1.0",
    "nyc": "^14.0.0",
    "semantic-release": "^15.13.21"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "snyk": true
}