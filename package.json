{
  "name": "helix-static",
  "version": "1.0.1",
  "private": true,
  "description": "Helix micro service for serving static files from GitHub",
  "main": "src/index.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "semantic-prepare": "node ./build/prepare.js",
    "commit": "git-cz",
    "lint": "eslint .",
    "test": "nyc mocha && npm run lint",
    "test-ci": "npm run lint && nyc mocha --reporter mocha-junit-reporter && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "wsk-builder -v",
    "deploy": "wsk-builder -v --deploy --test"
  },
  "wsk": {
    "name": "helix-services/static@1.0.1",
    "static": [
      ".env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/adobe/helix-static.git"
  },
  "keywords": [
    "helix"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-static/issues"
  },
  "homepage": "https://github.com/adobe/helix-static#readme",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "mime-types": "^2.1.24",
    "object-hash": "^1.3.1",
    "postcss": "^7.0.16",
    "postcss-url": "^8.0.0",
    "postcss-value-parser": "^3.3.1",
    "sanitizer": "^0.1.3",
    "uri-js": "^4.2.2"
  },
  "devDependencies": {
    "@adobe/openwhisk-action-builder": "^1.0.1",
    "@pollyjs/adapter-node-http": "^2.4.0",
    "@pollyjs/core": "^2.4.0",
    "@pollyjs/persister-fs": "^2.1.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "ajv": "^6.5.3",
    "archiver": "^3.0.0",
    "chalk": "^2.4.1",
    "codecov": "^3.1.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "ghooks": "^2.0.4",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.18.0",
    "nock": "^10.0.6",
    "nyc": "^14.0.0",
    "semantic-release": "^15.13.3"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}